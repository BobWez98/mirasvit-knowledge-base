{
    "name": "rapidez/mirasvit-knowledge-base",
    "description": "Rapidez Mirasvit Knowledge Base",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "authors": [
        {
            "name": "Roy Duineveld",
            "email": "royduineveld@gmail.com",
            "homepage": "https://royduineveld.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:rapidez/core.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "rapidez/core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\MirasvitKnowledgeBase\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\MirasvitKnowledgeBase\\MirasvitKnowledgeBaseServiceProvider"
            ]
        }
    }
}
